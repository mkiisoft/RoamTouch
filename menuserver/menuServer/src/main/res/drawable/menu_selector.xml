<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<!-- touch down -->
	<item>
	    <shape>
	    	<stroke android:color="@color/menu_row_line_not_selected" />
	    	<solid android:color="@color/menu_row_not_selected" />
	    </shape>
	</item>
	 <!-- selected -->
	<item android:state_focused="true">
	     <shape>
	     	<stroke android:color="@color/menu_row_line_selected" />
	    	<solid android:color="@color/menu_row_selected" />
	    </shape>
	</item>
	<!-- ormal -->
	<item>
	     <shape>
	     	<stroke android:color="@color/menu_row_line_normal" />
	    	<solid android:color="@color/menu_row_normal" />
	    </shape>
	</item>
</selector>